# Copy this file to .env and fill in your keys
# Vite exposes only variables prefixed with VITE_

# Get your OMDB API key from: https://www.omdbapi.com/apikey.aspx
VITE_OMDB_API_KEY=

# Get your Clerk Publishable Key from: https://dashboard.clerk.com
# Optional: if you plan to use authentication
VITE_CLERK_PUBLISHABLE_KEY=

# Get your TasteDive API key from: https://tastedive.com/read/api
VITE_TASTEDIVE_API_KEY=

# Get your Trakt.tv API key from: https://trakt.tv/oauth/applications
VITE_TRAKT_API_KEY=

# Get your TMDB API key from: https://www.themoviedb.org/settings/api
VITE_TMDB_API_KEY=
